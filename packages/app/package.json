{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run test && vite",
    "start": "npm run test && vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:watch": "vitest",
    "test": "vitest run --retry 1",
    "test:ui": "vitest --ui",
    "test:coverage-ci": "vitest run --coverage --retry 1",
    "test:coverage": "vitest --coverage",
    "lhci": "lhci autorun",
    "lint": "eslint --ext .js,.vue --fix src",
    "format": "prettier .  --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.14",
    "tailwindcss": "^4.0.14",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "@vue/test-utils": "^2.4.6",
    "c8": "^10.1.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "lighthouse": "^12.5.0",
    "lint-staged": "^15.5.0",
    "prettier": "3.5.3",
    "vite": "^6.3.6",
    "vitest": "^3.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run coverage"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{css,scss,md}": [
      "npm run format"
    ]
  }
}
